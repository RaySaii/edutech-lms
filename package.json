  "scripts": {
    "dev:services": "docker compose -f docker-compose.dev.yml up -d",
    "dev:frontend": "nx serve frontend",
    "dev:api-gateway": "nx serve api-gateway",
    "dev:auth-service": "nx serve auth-service",
    "dev:course-service": "nx serve course-service",
    "dev:all": "nx run-many --target=serve --projects=api-gateway,auth-service,course-service,user-service,notification-service,analytics-service --parallel",
    "test:unit": "nx run-many --target=test --all",
    "test:e2e": "nx run-many --target=e2e --all",
    "test:watch": "nx test frontend --watch",
    "test:e2e:watch": "nx e2e frontend-e2e --ui",
    "test:coverage": "nx test frontend --coverage",
    "test:all": "npm run test:unit && npm run test:e2e",
    "lint": "nx run-many --target=lint --all",
    "build": "nx run-many --target=build --all",
    "build:prod": "nx run-many --target=build --all --prod"
  },
